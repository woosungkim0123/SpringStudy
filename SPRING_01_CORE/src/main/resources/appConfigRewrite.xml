<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

        <bean id="memberService" class="basic.rewrite.member.MemberServiceRewriteImpl">
            <constructor-arg name="memberRepositoryRewrite" ref="memberRepository" />
        </bean>

        <bean id="memberRepository" class="basic.rewrite.member.MemoryMemberRepositoryRewrite" />

        <bean id="orderService" class="basic.rewrite.order.OrderServiceRewriteImpl">
            <constructor-arg name="memberRepository" ref="memberRepository" />
            <constructor-arg name="discountPolicy" ref="discountPolicy" />
        </bean>

        <bean id="discountPolicy" class="basic.rewrite.discount.RateDiscountPolicyRewrite" />

</beans>